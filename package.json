{
  "name": "progressive-blur",
  "description": "Drop-in progressive blur for React.",
  "keywords": [
    "react",
    "ui",
    "blur",
    "gradient",
    "progressive"
  ],
  "license": "MIT",
  "author": "Andrew Prifer (https://github.com/AndrewPrifer)",
  "repository": "your-username/progressive-blur",
  "bugs": "https://github.com/your-username/progressive-blur/issues",
  "homepage": "https://github.com/your-username/progressive-blur",
  "version": "1.0.1",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir lib",
    "build-vite": "vite build"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "framer-motion": "^10.18.0",
    "leva": "^0.9.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.2.2",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}
